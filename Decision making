#Greatest of two numbers
a=int(input())
b=int(input())
if a==b:
   print(a,"is equal to", b)
elif a>b:
   print(a,"is greater than", b)
else:
   print(a,"is less than ",b)
    
#Vowel Or Consonants
a=input()
vowels="aeiouAEIOU"
if a>='a' and a<='z' or a>='A' and a<='Z':
    if a in vowels:
        print("vowel")
    else:
        print("consonant")
else:
    print("Not an alphabet")
    
  #Grading System
   m=int(input())
if(m==100):
    print('S')
elif(90<=m<100):
    print('A')
elif(80<=m<90):
    print('B')
elif(70<=m<80):
    print('c')
elif(60<=m<70):
    print('D')
elif(50<=m<60):
    print('E')
elif(m<50):
    print('F')
else:
    print("invalid input")
    
    #Profit or Loss
    cp=float(input())
spofonemango=float(input())
sp=spofonemango*12
print("Enter the price of a dozen mangoes")
print("Enter the price at which 1 mango is being sold")
if sp==cp:
    print("No profit nor Loss")
elif sp>cp:
    print("profit:Rs.%.2f"%(sp-cp))
else:
    print("Loss:Rs.%.2f"%(cp-sp)) 
    
#Fee Collection
print("Enter the student type")
s=input()
if s=="MSDS":
    print("Enter tuition fee")
    T=float(input())
    print("Enter bus fee")
    B=float(input())
    x=T+B
    print("The fees to be paid by the student is Rs.%.2f"%x)
elif s=="MSH":
    print("Enter tuition fee")
    T=float(input())
    print("Enter hostel fee")
    H=float(input())
    y=T+H
    print("The fees to be paid by the student is Rs.%.2f"%y)
elif s=="MGSDS":
    print("Enter tuition fee")
    T=float(input())
    print("Enter bus fee")
    B=float(input())
    z=((150/100)*T)+B
    print("The fees to be paid by the student is Rs.%.2f"%z)
elif s=="MGSH":
    print("Enter tuition fee")
    T=float(input())
    print("Enter hostel fee")
    H=float(input())
    p=((150/100)*T)+H
    print("The fees to be paid by the student is Rs.%.2f"%p)
    
    #Y2K Problem Detector
    by=int(input("Enter Year of Birth\n"))
cy=int(input("Enter Current Year\n"))
if cy>=by:
    print(f"Your age is {cy-by}")
else:
    print(f"Your age is {100+cy-by}")
    
#Lab Allocation
   print("Enter x")
x=int(input())
print("Enter y")
y=int(input())
print("Enter z")
z=int(input())
if x<y and x<z:
      print("L1 has the minimal seating capacity")
elif y<x and y<z:
      print("L2 has the minimal seating capacity")
else:
      print("L3 has the minimal seating capacity")
      
 #Gardening I
 R=int(input())
