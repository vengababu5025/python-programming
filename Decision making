N=int(input())
if N<=C and N>=1:
    print("Yes")
elif N%C==1:
    print("Yes")
else:
    print("No")
    
 #Hotel Tariff Calculator
 m=int(input())
r=float(input())
d=int(input())
if m>0 and m<=12:
    if m in [4,5,6,11,12]:
        print(f"Hotel Tariff: Rs.{'%.2f'%(1.2*r*d)}")
    else:
         print(f"Hotel Tariff: Rs.{'%.2f'%(r*d)}")
else:
    print("Invalid Input")
    
